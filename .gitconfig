[user]
	name = Valentin Volkl
	email = valentin.volkl@cern.ch
	github = vvolkl
[push]
	default = current

[alias]
  co = checkout
  ci = commit
  st = status -s
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[diff]
	tool = meld
